{
    "name": "hachirobei/npp-rabbitmq-publisher",
    "description": "A Laravel package for RabbitMQ integration",
    "type": "library",
    "require": {
        "php": ">=7.5",
        "illuminate/support": "~5.5|~6.0|~7.0|~8.0|~9.0|^10.0",
        "php-amqplib/php-amqplib":  ">=3"
    },
    "autoload": {
        "psr-4": {
            "hachirobei\\NPPRabbitMQPublisher\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "hachirobei\\NPPRabbitMQPublisher\\RabbitMQServiceProvider"
